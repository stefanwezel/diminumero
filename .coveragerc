[run]
source = .
omit = 
    tests/*
    .venv/*
    venv/*
    */site-packages/*
    generate_numbers.py
    */tests/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
